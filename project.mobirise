->\n    <!-- Overlay -->\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <!-- Container -->\n    <div class=\"footer-container\" mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth, 'mbr-jc-c': (!showCopyright || !showItems) && typeBlock==2, 'mbr-flex': typeBlock==2, 'mbr-row-reverse': reverseContent}\">\n        <div mbr-list mbr-list-grow=\"itemsCount\" class=\"mbr-row link-items mbr-jc-c mbr-fonts-style\" mbr-if=\"showItems\" mbr-theme-style=\"display-4\">\n            <p class=\"fLink\" data-app-selector=\".fLink\"><a href=\"index.html\">Home</a></p>\n            <p class=\"fLink\" data-app-selector=\".fLink\">Produk</p>\n            <p class=\"fLink\" data-app-selector=\".fLink\">Berita</p>\n            <p class=\"fLink\" data-app-selector=\".fLink\"><a href=\"komunitas.html\">Komunitas</a></p>\n            <p class=\"fLink\" data-app-selector=\".fLink\"><a href=\"legalitas.html\">Legalitas</a></p>\n        <p class=\"fLink\" data-app-selector=\".fLink\"><a href=\"pendaftaran.html\">Pendaftaran</a></p><p class=\"fLink\" data-app-selector=\".fLink\"><a href=\"hubungi.html\">Kontak</a></p></div>\n        <div class=\"copyright mbr-flex mbr-jc-c\" mbr-if=\"showCopyright\">\n            <p class=\" mbr-fonts-style align-center\" mbr-theme-style=\"display-4\" data-app-selector=\".copyright\" data-app-placeholder=\"Type Text\">© Copyright 2019 kopahindo.com – All Rights Reserved</p>\n        </div>\n    </div>\n</section>",
